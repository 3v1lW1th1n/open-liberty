-include= ~../cnf/resources/bnd/bundle.props
bVersion=1.0

javac.source: 1.8
javac.target: 1.8

Bundle-Name: Config REST API
Bundle-SymbolicName: com.ibm.ws.rest.handler.config
Bundle-Description: REST API for server configuration; version=${bVersion}

Require-Capability: osgi.ee;filter:="(&(osgi.ee=JavaSE)(version=1.8))"

WS-TraceGroup: config.rest

Web-ContextPath: bogus/root
OL-VirtualHost: ${admin.virtual.host}

IBM-Authorization-Roles: com.ibm.ws.management
IBM-Web-Extension-Processing-Disabled: true

Private-Package:\
  com.ibm.ws.rest.handler.config.*
  
#Import-Package: com.ibm.wsspi.rest.handler,\
#  com.ibm.wsspi.rest.handler.helper,\
#  com.ibm.ws.rest.handler.helper,\
#  *

Include-Resource: \
    WEB-INF=resources/WEB-INF
    
-dsannotations:\
  com.ibm.ws.rest.handler.config.internal.ConfigRESTHandler

-buildpath:\
  com.ibm.json4j;version=latest,\
  com.ibm.websphere.appserver.spi.kernel.service,\
  com.ibm.websphere.appserver.spi.logging,\
  com.ibm.websphere.org.osgi.core,\
  com.ibm.websphere.org.osgi.service.cm,\
  com.ibm.websphere.org.osgi.service.component,\
  com.ibm.websphere.rest.handler;version=latest,\
  com.ibm.wsspi.org.osgi.service.component.annotations,\
  com.ibm.wsspi.org.osgi.service.metatype.annotations;version=latest,\
  com.ibm.ws.config;version=latest,\
  com.ibm.ws.container.service;version=latest,\
  com.ibm.ws.bnd.annotations;version=latest,\
  com.ibm.ws.org.apache.felix.scr;version=latest,\
  com.ibm.ws.org.osgi.annotation.versioning;version=latest,\
  com.ibm.websphere.javaee.servlet.3.0;version=latest,\
  com.ibm.ws.rest.handler;version=latest
